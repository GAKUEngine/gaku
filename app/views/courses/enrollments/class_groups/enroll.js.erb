<% @not_added_students.each {|enrollment| %>
  $('#enrolled_students_grid tbody').append('<%= escape_javascript(render :partial => "courses/enrollments/enrolled", :formats => [:html], :handlers => [:erb, :slim], :locals => {:enrollment => enrollment}) %>');
<% } %>
$('#add_class_group_enrollment_link').show();
var flashError = "<%=flash.now[:error]%>";
if (flashError){
  $(".flash-error").html(flashError);
}else{
  $(".flash-error").html("");
}