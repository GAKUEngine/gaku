<h1>Listing course_enrollments</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Course</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @course_enrollments.each do |course_enrollment| %>
  <tr>
    <td><%= course_enrollment.student %></td>
    <td><%= course_enrollment.course %></td>
    <td><%= link_to 'Show', course_enrollment %></td>
    <td><%= link_to 'Edit', edit_course_enrollment_path(course_enrollment) %></td>
    <td><%= link_to 'Destroy', course_enrollment, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

