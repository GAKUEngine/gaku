<% @not_added_students.each {|enrollment| %>
  $('#course-students-index tbody').append('<%= escape_javascript(render :partial => "courses/enrollments/enrolled", :formats => [:html], :handlers => [:erb, :slim], :locals => {:enrollment => enrollment}) %>');
<% } %>
$('#new-course-class-group-link').show();
var flashError = "<%=flash.now[:error]%>";
if (flashError){
  $(".flash-error").html(flashError);
}else{
  $(".flash-error").html("");
}