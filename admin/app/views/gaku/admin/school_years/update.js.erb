<%- if @school_year.valid? %>
  $('tr#school-year-<%= @school_year.id %>').html('<%= j render("fields", {school_year: @school_year}) %>');
  $("#edit-school-year-modal").modal("hide");
  showNotice("<%= render_flash %>");
  $('#edit-school-year-modal-errors').html('')
<%- else %>
    $('#edit-school-year-modal-errors').html('<%= j render("gaku/shared/errors", {target: @school_year}) %>')
<% end %>
