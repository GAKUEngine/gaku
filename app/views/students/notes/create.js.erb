$('#student-notes-index tbody').append('<%= escape_javascript(render :partial => "note", :formats => [:html], :handlers => [:erb, :slim], :locals => {:note => @student.notes.last}) %>');
$('#new-student-note-form form').each(function() {
	this.reset();
});

$('#new-student-note-form').html('');
$('#new-student-note-link').show();

$('#new-student-note-tab-link').html('<%= print_count(@student.notes, t('notes.notes')) %>');
$('.student-notes-count').html('<%= print_count(@student.notes, t('notes.list')) %>');
