<% if @grading_method_connectors && @grading_method_connectors.any? %>
  $('#<%= @gradable_resource %>-grading-method-connectors-index tbody')
    .append('<%= j render(partial: "grading_method_connector", collection: @grading_method_connectors, as: :grading_method_connector) %>');

  $('.<%= @gradable_resource %>-grading-method-connectors-count').html("<%= print_count(@count, t(:'grading_method.list')) %>");
  $('#<%= @gradable_resource %>-grading-method-connectors-tab-link').html("<%= print_count(@count, t(:'grading_method.list')) %>");

<% end %>
showNotice("<%= render_flash %>");
$('#new-set-<%= @gradable_resource %>-grading-method-connector').slide()
$('#new-set-<%= @gradable_resource %>-grading-method-connector-link').show()

