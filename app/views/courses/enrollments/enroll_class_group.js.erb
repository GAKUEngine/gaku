<% @not_added_students.each {|enrollment| %>
  $('#enrolled_students_grid tbody').append('<%= escape_javascript(render :partial => "enrolled", :formats => [:html], :handlers => [:erb, :slim], :locals => {:enrollment => enrollment}) %>');
<% } %>
var flashError = "<%=flash.now[:error]%>";
if (flashError){
  $(".flash-error").html(flashError);
}else{
	$(".flash-error").html("");
}