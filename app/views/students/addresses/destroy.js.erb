$('#new-student-address-tab-link').html('<%= @student.addresses.count != 0 ? t('addresses.addresses') + "（" + @student.addresses.count.to_s + "）" : t('addresses.addresses') %>');
$('.student-addresses-count').html('<%= @student.addresses.count != 0 ? t('addresses.list') + "（" + @student.addresses.count.to_s + "）" : t('addresses.list') %>');
<%- unless @student.addresses.blank? %>
	$('#address-<%= @student.addresses.first.id%> td a').addClass('btn-primary')
<% end %>
