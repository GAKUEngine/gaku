<%- if @semester.valid? %>
  $('#admin-school-year-semesters-index tbody').append('<%= j render("semester", {semester: @semester}) %>');
  $('#new-admin-school-year-semester').slide();
  $('#new-admin-school-year-semester-link').show();
  showNotice("<%= render_flash %>");
  $('.admin-school-year-semesters-count').html("<%= print_count(@count, t(:'semester.list')) %>");
  $('#new-admin-school-year-semester-errors').html(' ')
<%- else %>
  $('#new-admin-school-year-semester-errors').html('<%= j render("gaku/shared/errors", {target: @semester}) %>')
<% end %>