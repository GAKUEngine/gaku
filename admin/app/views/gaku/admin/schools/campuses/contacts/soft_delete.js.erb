showNotice("<%= render_flash %>");

$('#admin-school-campus-contacts-tab-link').html("<%= print_count(@count, t(:'contact.plural')) %>");
$(".admin-school-campus-contacts-count").html("<%= print_count(@count, t(:'contact.list')) %>");

<%- if @campus.contacts.reload.any? & @primary_contact %>
  $('#contact-<%= @campus.contacts.first.id %> td a').addClass('btn-primary');
<% end %>