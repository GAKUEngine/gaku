$('#student-notes-index tbody').append('<%= render_js_partial("note", {:note => @student.notes.last}) %>');
$('#new-student-note form').each(function() {
	this.reset();
});

$('#new-student-note').slideToggle();
$('#new-student-note-link').show();

$('#new-student-note-tab-link').html('<%= print_count(@student.notes.count, t('notes.notes')) %>');
$('.student-notes-count').html('<%= print_count(@student.notes.count, t('notes.list')) %>');
showNotice("<%= render_flash %>");
