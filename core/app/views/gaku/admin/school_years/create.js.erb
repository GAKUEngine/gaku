<%- if @school_year.valid? %>
  $('#admin-school-years-index tbody').append('<%= j render("school_year", {school_year: @school_year}) %>');
  $('#new-admin-school-year').slide();
  $('#new-admin-school-year-link').show();
  showNotice("<%= render_flash %>");
  $('.admin-school-years-count').html("<%= print_count(@count, t(:'school_year.list')) %>");
  $('#new-admin-school-year-errors').html('')
<%- else %>
   $('#new-admin-school-year-errors').html('<%= j render("gaku/shared/errors", {target: @school_year}) %>')
<% end %>